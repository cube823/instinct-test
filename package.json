{
  "name": "instinct-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx wrangler pages dev --port 3000 -- npx next dev --port 3001",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "preview": "npm run build && npx wrangler pages dev out --port 3000",
    "d1:migrate:local": "npx wrangler d1 migrations apply instinct-test-db --local",
    "d1:migrate:prod": "npx wrangler d1 migrations apply instinct-test-db --remote",
    "generate:og": "npx tsx scripts/generate-og-images.ts"
  },
  "dependencies": {
    "framer-motion": "^12.34.0",
    "html-to-image": "^1.11.13",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/sharp": "^0.31.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "satori": "^0.19.2",
    "sharp": "^0.34.5",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4"
  }
}
